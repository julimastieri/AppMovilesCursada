<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moviles">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Clase2SegundaActivity"></activity>
        <activity android:name=".Clase4" />
        <activity android:name=".Clase3" />
        <activity
            android:name=".Clase2"
            android:label="@string/title_activity_clase2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Clase1"
            android:label="@string/title_activity_clase1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <service android:name=".C4IntentService" />

    </application>

</manifest>